<% obj.each do |bill_of_lading| %>
  <tr>
    <td width="25%"><%= bill_of_lading.document_number %></td>
    <td width="25%"><%= bill_of_lading.created_at.strftime("%B %d %Y %l:%M%p") %></td>
    <td width="25%"><%= bill_of_lading.updated_at.strftime("%B %d %Y %l:%M%p") %></td>
    
    <td width="8%"> <%= link_to 'Edit', edit_bill_of_lading_path(bill_of_lading) %></td>
    <td width="8%"> <%= link_to 'View', bill_of_lading_path(bill_of_lading) %></td>
    <td width="8%"> <%= link_to 'Delete', bill_of_lading_path(bill_of_lading), method: :delete, data: {confirm: "Are you sure you want to delete this B/L?"} %></td>
  </tr>
<% end %>